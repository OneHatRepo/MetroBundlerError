{
  "name": "testapp",
  "version": "1.0.0",
  "scripts": {
    "esd": "cp .env.dev .env && EXPO_DEBUG=1 expo start --dev-client -c",
    "ebp": "cp .env.production .env && eas build --profile production --auto-submit",
    "ebs": "cp .env.staging .env && eas build --profile staging",
    "ebt": "cp .env.testing .env && eas build --profile testing",
    "ebd": "cp .env.dev .env && eas build --profile dev",
    "start": "expo start --dev-client",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "eject": "expo eject"
  },
  "dependencies": {
    "dotenv": "^16.0.1",
    "expo": "~45.0.0",
    "expo-community-flipper": "^45.1.0",
    "expo-dev-client": "~1.0.0",
    "expo-splash-screen": "~0.15.1",
    "expo-status-bar": "~1.3.0",
    "expo-tracking-transparency": "~2.2.0",
    "react": "17.0.2",
    "react-devtools-core": "^4.24.7",
    "react-dom": "17.0.2",
    "react-native": "0.68.2",
    "react-native-flipper": "^0.152.0",
    "react-native-reanimated": "~2.8.0",
    "react-native-web": "0.17.7",
    "expo-updates": "~0.13.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.18.6",
    "@expo/metro-config": "^0.3.18"
  },
  "private": true
}
